{
  "expo": {
    "name": "Bill Splitter",
    "slug": "bill-splitter",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.billsplitter.app",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "en",
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.billsplitter.app",
      "versionCode": 1,
      "allowBackup": false
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "da76841c-5ca4-4a5a-a650-e75e17c3fe61"
      }
    }
  }
}
